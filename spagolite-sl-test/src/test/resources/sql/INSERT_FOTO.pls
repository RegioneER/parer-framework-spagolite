create or replace PROCEDURE INSERT_FOTO
(ID_OGGETTO_EVENTO IN NUMBER, FOTO IN CLOB, VALORE OUT NUMBER) IS
  
 BEGIN
    SELECT SLOG_FOTO_OGGETTO_EVENTO.nextval 
    INTO VALORE 
    FROM DUAL;

    INSERT INTO LOG_FOTO_OGGETTO_EVENTO (ID_FOTO_OGGETTO_EVENTO, ID_OGGETTO_EVENTO, BL_FOTO_OGGETTO) VALUES(VALORE, ID_OGGETTO_EVENTO, XMLTYPE(FOTO)); 

 END INSERT_FOTO;